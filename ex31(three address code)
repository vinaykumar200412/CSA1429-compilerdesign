#include <stdio.h>

int main() {
    int a, b, c, d, result;

    // Read input values
    printf("Enter values for a and b: ");
    scanf("%d %d", &a, &b);

    // Perform operations
    c = a + b;
    d = a - b;
    result = c * d;

    // Display result
    printf("Result: %d\n", result);

    return 0;
}
